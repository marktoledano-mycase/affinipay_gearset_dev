<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Brand_Name__c</fullName>
    <description>Converts brand name into record type name for brand on opportunities</description>
    <externalId>false</externalId>
    <formula>CASE( Brand__c,

&quot;lawpay&quot;, &quot;LawPay Payments&quot;,
&quot;cpacharge&quot;, &quot;CPACharge&quot;,
&quot;affinipay&quot;, &quot;AffiniPay&quot;,
&quot;clientpay&quot;, &quot;ClientPay&quot;,

&quot;LawPay Payments&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Converts brand name into record type name for brand on opportunities</inlineHelpText>
    <label>Brand Name</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
