<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tier_Status__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
   ( User_Count_Rollup__c &gt; 0 &amp;&amp; TEXT( SKU_Product__r.Product_Type__c) = &quot;MAIN&quot;),
   &quot;Active Tier&quot;,
   &quot;&quot;
   )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Displays &quot;Active Tier&quot; when the Product Type on the SKU Product is Main and the User Count rollup field is greater than 0</inlineHelpText>
    <label>Tier Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
