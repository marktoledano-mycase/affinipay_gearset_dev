<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GiactBusinessChangeFromAutoApproval</fullName>
    <active>false</active>
    <errorConditionFormula>AND( 
ISCHANGED(GIACTBusinessValidation__c), 
TEXT(PRIORVALUE(GIACTBusinessValidation__c))= &#39;Automated Approval&#39; 
)</errorConditionFormula>
    <errorMessage>You are not authorized to change the &#39;GIACT Business Validation&#39; from &#39;Automated Approval&#39;</errorMessage>
</ValidationRule>
