<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Monthly_Pacing__c</fullName>
    <description>TRUE if day of opportunity within the month it was created is less or equal to today</description>
    <externalId>false</externalId>
    <formula>IF( DAY(Account.Conversion_Date__c) &lt;= DAY(TODAY()), TRUE, FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Monthly Pacing</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
