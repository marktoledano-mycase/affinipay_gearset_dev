<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valid_Routing_Number</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(Regex(RoutingNo__c, &quot;^[0-9]{9}$&quot;))</errorConditionFormula>
    <errorDisplayField>RoutingNo__c</errorDisplayField>
    <errorMessage>Please enter a valid routing number.</errorMessage>
</ValidationRule>
