<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Maximum_Sales_Discount_Exceeded__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
   Maximum_SKU_Product_Discount__c &lt;   Promotion_and_Extended_Discount__c ,
   TRUE,
   FALSE
   )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Identifies the condition that the percentage discount extended on the record exceeds the Maximum Sales Discount on the SKU Product.</inlineHelpText>
    <label>Maximum Sales Discount Exceeded</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
