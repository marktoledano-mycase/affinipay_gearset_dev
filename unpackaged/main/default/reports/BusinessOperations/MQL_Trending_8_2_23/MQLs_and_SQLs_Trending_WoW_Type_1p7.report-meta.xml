        <developerName>FORMULA2</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>SQL Win Rate</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>RowCount / 

((
PREVGROUPVAL(RowCount,Trial_Request__c.MQL_Date__c,1)+
PREVGROUPVAL(RowCount,Trial_Request__c.MQL_Date__c,2)+
PREVGROUPVAL(RowCount,Trial_Request__c.MQL_Date__c,3)+
PREVGROUPVAL(RowCount,Trial_Request__c.MQL_Date__c,4)+
PREVGROUPVAL(RowCount,Trial_Request__c.MQL_Date__c,5)+
PREVGROUPVAL(RowCount,Trial_Request__c.MQL_Date__c,6)

)/7)</calculatedFormula>
        <datatype>percent</datatype>
        <developerName>FORMULA3</developerName>
        <downGroupingContext>Trial_Request__c.MQL_Date__c</downGroupingContext>
        <isActive>false</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>MQL Performance</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>CDF1:SUM / ((PARENTGROUPVAL(RowCount, GRAND_SUMMARY) - CDF1:SUM)/7)</calculatedFormula>
