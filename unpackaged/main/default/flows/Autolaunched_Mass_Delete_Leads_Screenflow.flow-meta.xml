    <decisions>
        <name>Are_there_too_many_selected_leads</name>
        <label>Are there too many selected leads?</label>
        <locationX>226</locationX>
        <locationY>542</locationY>
        <defaultConnector>
            <targetReference>Has_Error</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Leads are within allowed amount</defaultConnectorLabel>
        <rules>
            <name>Too_many_leads</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_Selectedleadcount</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>50.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_too_many_leads_flag</targetReference>
            </connector>
            <label>Too many leads</label>
        </rules>
    </decisions>
    <decisions>
        <description>Evaluates whether the running user has the &quot;Mass Delete Leads&quot; permission set.</description>
        <name>Evaluate_Permission_Set</name>
        <label>Evaluate Permission Set</label>
        <locationX>226</locationX>
        <locationY>134</locationY>
        <defaultConnector>
            <targetReference>Count_Records</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Does not have Permission Set</defaultConnectorLabel>
        <rules>
            <name>Has_Permission_Set</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Permission.Mass_Delete_Leads</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Authorized_User</targetReference>
            </connector>
            <label>Has Permission Set</label>
        </rules>
    </decisions>
    <decisions>
        <name>Has_Error</name>
