    <formulas>
        <name>fx_LastMQABrand</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!Get_Account.Last_MQA_Brand__c}, &quot;LawPay&quot;), &quot;affinipay&quot;,
    IF(ISPICKVAL({!Get_Account.Last_MQA_Brand__c}, &quot;MyCase&quot;), &quot;MyCase&quot;, &quot;&quot;))</expression>
    </formulas>
    <formulas>
        <name>fx_OwnerName</name>
        <dataType>String</dataType>
        <expression>IF(
ISPICKVAL({!Get_Assignment_Settings.var_AssignmentType}, &quot;Assignment Group&quot;), 
{!Get_Assignment_Group_Member.var_AssignmentGroupMemberUserName}, 
{!Get_User.Name}
)</expression>
    </formulas>
    <formulas>
        <name>fx_PaymentCustomerStatus</name>
        <dataType>String</dataType>
        <expression>IF(
    ISPICKVAL({!Get_Account.AffiniPay_Customer_Status__c}, &quot;Active&quot;) ||
    ISPICKVAL({!Get_Account.ClientPay_Customer_Status__c}, &quot;Active&quot;) ||
    ISPICKVAL({!Get_Account.CPACharge_Customer_Status__c}, &quot;Active&quot;) ||
    ISPICKVAL({!Get_Account.LawPay_Customer_Status__c}, &quot;Active&quot;),
    &quot;Active&quot;,

IF(
    ISPICKVAL({!Get_Account.AffiniPay_Customer_Status__c}, &quot;Churned&quot;) ||
    ISPICKVAL({!Get_Account.ClientPay_Customer_Status__c}, &quot;Churned&quot;) ||
    ISPICKVAL({!Get_Account.CPACharge_Customer_Status__c}, &quot;Churned&quot;) ||
    ISPICKVAL({!Get_Account.LawPay_Customer_Status__c}, &quot;Churned&quot;),
    &quot;Churned&quot;,

IF(
    ISPICKVAL({!Get_Account.AffiniPay_Customer_Status__c}, &quot;Prospect&quot;) ||
    ISPICKVAL({!Get_Account.ClientPay_Customer_Status__c}, &quot;Prospect&quot;) ||
    ISPICKVAL({!Get_Account.CPACharge_Customer_Status__c}, &quot;Prospect&quot;) ||
    ISPICKVAL({!Get_Account.LawPay_Customer_Status__c}, &quot;Prospect&quot;),
    &quot;Prospect&quot;,&quot;Prospect&quot;)))</expression>
    </formulas>
    <formulas>
        <name>fx_SoftwareCustomerStatus</name>
        <dataType>String</dataType>
        <expression>IF(
    ISPICKVAL({!Get_Account.CASEpeer_Customer_Status__c}, &quot;Active&quot;) ||
    ISPICKVAL({!Get_Account.Docketwise_Customer_Status__c}, &quot;Active&quot;) ||
    ISPICKVAL({!Get_Account.MyCase_Customer_Status__c}, &quot;Active&quot;),
    &quot;Active&quot;,

IF(
    ISPICKVAL({!Get_Account.CASEpeer_Customer_Status__c}, &quot;Churned&quot;) ||
    ISPICKVAL({!Get_Account.Docketwise_Customer_Status__c}, &quot;Churned&quot;) ||
    ISPICKVAL({!Get_Account.MyCase_Customer_Status__c}, &quot;Churned&quot;),
    &quot;Churned&quot;,

IF(
    ISPICKVAL({!Get_Account.CASEpeer_Customer_Status__c}, &quot;Prospect&quot;) ||
    ISPICKVAL({!Get_Account.Docketwise_Customer_Status__c}, &quot;Prospect&quot;) ||
    ISPICKVAL({!Get_Account.MyCase_Customer_Status__c}, &quot;Prospect&quot;),
    &quot;Prospect&quot;,&quot;Prospect&quot;)))</expression>
    </formulas>
