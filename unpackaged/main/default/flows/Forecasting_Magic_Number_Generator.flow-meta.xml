    <description>Generates prediction for opps created in remaining days of the month based on prior 3 months to calculate deals and users won</description>
    <environments>Default</environments>
    <formulas>
        <name>f_AverageUsersField</name>
        <dataType>String</dataType>
        <expression>IF( {!varTestMode}, 
&quot;Forecasted_Users__c&quot;, 
&quot;Won_Users__c&quot;
)</expression>
    </formulas>
    <formulas>
        <name>f_DayToday</name>
        <dataType>Number</dataType>
        <expression>DAY({!$Flow.CurrentDate})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_ProjectedARR_Software</name>
        <dataType>Currency</dataType>
        <expression>{!f_ProjectedUsersWonRemaining} * 79 * 12</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_ProjectedARR_won</name>
        <dataType>String</dataType>
        <expression>&quot;$&quot; &amp; TEXT({!f_ProjectedUsersWonRemaining} * 79 * 12)</expression>
    </formulas>
    <formulas>
        <name>f_ProjectedDealsWonSoftware</name>
        <dataType>Number</dataType>
        <expression>{!ProjectedDealsWonCount} / 3</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_ProjectedLogosWonPayments</name>
        <dataType>Number</dataType>
        <expression>{!ProjectedDealsWonPayments} / 3</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_ProjectedOppsCreatedPayments</name>
        <dataType>Number</dataType>
        <expression>{!ProjectedOppsPaymentsCount} / 3</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_ProjectedOppsCreatedSoftware</name>
        <dataType>Number</dataType>
        <expression>{!ProjectedOppCount} / 3</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_ProjectedUsersWonRemaining</name>
        <dataType>Number</dataType>
        <expression>{!AvgUsersWonPerDeal} * {!f_ProjectedDealsWonSoftware}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>first day 3 months ago</description>
        <name>f_threeMonthsAgo</name>
        <dataType>Date</dataType>
        <expression>DATE(
YEAR( ADDMONTHS( {!$Flow.CurrentDate}, -3)),
MONTH( ADDMONTHS( {!$Flow.CurrentDate}, -3)),
1
)</expression>
    </formulas>
    <formulas>
        <name>min_LastActivityDate</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} - 14</expression>
    </formulas>
