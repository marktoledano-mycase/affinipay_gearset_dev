    <collectionProcessors>
        <name>In_Month_Payments_Wins</name>
        <elementSubtype>FilterCollectionProcessor</elementSubtype>
        <label>In Month Payments Wins</label>
        <locationX>390</locationX>
        <locationY>837</locationY>
        <assignNextValueToReference>currentItem_In_Month_Payments_Wins</assignNextValueToReference>
        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>LawPay_Opportunities</collectionReference>
        <conditionLogic>formula_evaluates_to_true</conditionLogic>
        <connector>
            <targetReference>Assign_Payments_Won_Count</targetReference>
        </connector>
        <formula>AND( 
MONTH( {!currentItem_In_Month_Payments_Wins.Created_Date_fx__c} ) =  MONTH( {!currentItem_In_Month_Payments_Wins.CloseDate} ),
YEAR( {!currentItem_In_Month_Payments_Wins.Created_Date_fx__c} ) =  YEAR( {!currentItem_In_Month_Payments_Wins.CloseDate} ),
ISPICKVAL( {!currentItem_In_Month_Payments_Wins.StageName}, &quot;Closed Won&quot;)
)</formula>
    </collectionProcessors>
    <collectionProcessors>
        <name>In_Month_Wins</name>
        <elementSubtype>FilterCollectionProcessor</elementSubtype>
        <label>In Month Wins</label>
        <locationX>722</locationX>
        <locationY>567</locationY>
        <assignNextValueToReference>currentItem_In_Month_Wins_0</assignNextValueToReference>
        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>MyCase_Opportunities</collectionReference>
        <conditionLogic>formula_evaluates_to_true</conditionLogic>
        <connector>
            <targetReference>Assign_Win_Count</targetReference>
        </connector>
        <formula>IF( {!varTestMode},
AND
( 
MONTH( {!currentItem_In_Month_Wins_0.CloseDate} ) = MONTH( {!currentItem_In_Month_Wins_0.Created_Date_fx__c} ),
YEAR( {!currentItem_In_Month_Wins_0.CloseDate} ) = YEAR( {!currentItem_In_Month_Wins_0.Created_Date_fx__c} )
),
AND
(
ISPICKVAL( {!currentItem_In_Month_Wins_0.StageName}, &quot;Closed Won&quot;),
MONTH( {!currentItem_In_Month_Wins_0.CloseDate} ) = MONTH( {!currentItem_In_Month_Wins_0.Created_Date_fx__c} ),
YEAR( {!currentItem_In_Month_Wins_0.CloseDate} ) = YEAR( {!currentItem_In_Month_Wins_0.Created_Date_fx__c} )
))</formula>
    </collectionProcessors>
