                <rightValue>
                    <elementReference>$Record.Account.CPACharge_Customer_Status__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Docketwise_Customer_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Account.Docketwise_Customer_Status__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.LawPay_Customer_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>fmlaLawyPayPaymentsCustomerStatus</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.MyCase_Customer_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Account.MyCase_Customer_Status__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Soluno_Customer_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Account.Soluno_Customer_Status__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Websites_Customer_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Account.Websites_Customer_Status__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Woodpecker_Customer_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Account.Woodpecker_Customer_Status__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Contact_Brand_Customer_Status_Fields</targetReference>
            </connector>
            <label>Differences Detected</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>fmlaLawPayCustomerStatus</name>
        <dataType>String</dataType>
        <expression>IF(
    NOT( ISBLANK( {!fmlaLawyPayPaymentsCustomerStatus} )),
    {!fmlaLawyPayPaymentsCustomerStatus}, 
    IF( 
         NOT( ISBLANK( {!fmlaLayPaySoftwareCustomerStatus} )), 
         {!fmlaLayPaySoftwareCustomerStatus}, 
         &quot;&quot;
         )
     )</expression>
    </formulas>
    <formulas>
        <name>fmlaLawyPayPaymentsCustomerStatus</name>
        <dataType>String</dataType>
        <expression>IF(
    TEXT( {!$Record.Account.LawPay_Product_Tier__c} ) = &quot;LawPay Classic&quot;, 
    TEXT({!$Record.Account.LawPay_Customer_Status__c}),
    &quot;&quot;
    )</expression>
    </formulas>
    <formulas>
        <name>fmlaLayPaySoftwareCustomerStatus</name>
        <dataType>String</dataType>
        <expression>IF(
    TEXT( {!$Record.Account.LawPay_Product_Tier__c} ) = &quot;LawPay Pro&quot;, 
    TEXT({!$Record.Account.LawPay_Customer_Status__c}),
    &quot;&quot;
    )</expression>
    </formulas>
