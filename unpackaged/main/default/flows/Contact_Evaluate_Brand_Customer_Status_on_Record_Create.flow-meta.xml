        <defaultConnectorLabel>No Differences Detected</defaultConnectorLabel>
        <rules>
            <name>Differences_Detected</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.AffiniPay_Customer_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Account.AffiniPay_Customer_Status__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.CASEpeer_Customer_Status__c</leftValueReference>
