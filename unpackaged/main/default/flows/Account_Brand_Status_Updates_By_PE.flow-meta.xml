    <description>Process uses the PE records to calculate brand status on the account page</description>
    <environments>Default</environments>
    <formulas>
        <name>f_Affinipay_Status</name>
        <dataType>String</dataType>
        <expression>IF({!count_Affinipay_Total} &gt;0, 
   IF({!count_Affinipay_Active} &gt;0, &quot;Active&quot;, 
      IF({!count_Affinipay_Chruned} &gt;0, &quot;Churned&quot;, &quot;&quot;)),&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>f_ClientPay_Status</name>
        <dataType>String</dataType>
        <expression>IF({!count_ClientPay_Total} &gt;0, 
   IF({!count_ClientPay_Active} &gt;0, &quot;Active&quot;, 
      IF({!count_ClientPay_Chruned} &gt;0, &quot;Churned&quot;, &quot;&quot;)),&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>f_CPACharge_Status</name>
        <dataType>String</dataType>
