    <formulas>
        <name>f_FirmEmployees</name>
        <dataType>Number</dataType>
        <expression>IF( {!$Record.Number_of_Employees__c} &gt; 0, 
{!$Record.Number_of_Employees__c}, 
IF( {!Primary_Contact.Number_of_Employees__c} &gt; 0, 
{!Primary_Contact.Number_of_Employees__c}, 
IF( {!Primary_Contact.Account.NumberOfEmployees} &gt; 0, 
{!Primary_Contact.Account.NumberOfEmployees}, 
1
)))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>f_ForecastedUsers</name>
        <dataType>Number</dataType>
        <expression>IF( {!$Record.Forecasted_Users__c} &gt; 0, {!$Record.Forecasted_Users__c}, 
IF( {!Primary_Contact.Forecasted__c} &gt; 0, {!Primary_Contact.Forecasted__c}, 
1 ))</expression>
        <scale>0</scale>
    </formulas>
