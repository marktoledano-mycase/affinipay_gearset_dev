    <formulas>
        <name>fmlaAllPaymentBrands</name>
        <dataType>String</dataType>
        <expression>IF(
   OR(
        {!recOpportunity.Record_Type_Name__c} = &quot;Afinipay&quot;, 
        {!recOpportunity.Record_Type_Name__c} = &quot;ClientPay&quot;, 
        {!recOpportunity.Record_Type_Name__c} = &quot;CPACharge&quot;, 
        {!recOpportunity.Record_Type_Name__c} = &quot;LawPay&quot;
        ),
   &quot;All Payments&quot;,
   &quot;&quot;
   )</expression>
    </formulas>
    <formulas>
        <name>fmlaAllSoftwareBrands</name>
        <dataType>String</dataType>
        <expression>IF(
    OR(
         {!recOpportunity.Record_Type_Name__c} = &quot;CASEpeer&quot;,
         {!recOpportunity.Record_Type_Name__c} = &quot;Docketwise&quot;,
         {!recOpportunity.Record_Type_Name__c} = &quot;MyCase&quot;,
         {!recOpportunity.Record_Type_Name__c} = &quot;Soluno&quot;,
         {!recOpportunity.Record_Type_Name__c} = &quot;Woodpecker&quot;,
         {!recOpportunity.Record_Type_Name__c} = &quot;Websites&quot;
          ),
    &quot;All Software&quot;,
     &quot;&quot;
    )</expression>
    </formulas>
