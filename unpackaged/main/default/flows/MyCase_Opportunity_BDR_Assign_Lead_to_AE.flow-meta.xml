    <formulas>
        <name>MQL_date</name>
        <dataType>Date</dataType>
        <expression>IF( ISBLANK( {!Inbound_Request.MQL_Date__c} ), {!$Flow.CurrentDate}, {!Inbound_Request.MQL_Date__c} )</expression>
    </formulas>
    <formulas>
        <name>NumberOfEmployees</name>
        <dataType>Number</dataType>
        <expression>CASE( {!ObjectFormula},
&quot;Opportunity&quot;,
IF( ISBLANK( {!Opportunity.Professional_User_Count__c} ), 1, {!Opportunity.Professional_User_Count__c}),
IF( ISBLANK( {!Inbound_Request.Employees__c} ), 1, {!Inbound_Request.Employees__c})
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>ObjectFormula</name>
        <dataType>String</dataType>
        <expression>IF( LEFT({!recordId}, 3) = &quot;006&quot;, &quot;Opportunity&quot;, &quot;Inbound Request&quot;)</expression>
    </formulas>
