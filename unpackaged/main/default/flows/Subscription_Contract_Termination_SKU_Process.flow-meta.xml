        <dataType>Number</dataType>
        <expression>IF( {!Process_SKUs.User_Count_Rollup__c} &gt; 0, {!Process_SKUs.User_Count_Rollup__c} *-1,
{!Process_SKUs.Add_On_Users__c} * -1 )</expression>
        <scale>2</scale>
    </formulas>
