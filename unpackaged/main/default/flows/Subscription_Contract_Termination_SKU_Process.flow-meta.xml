    <formulas>
        <name>f_QuantityType</name>
        <dataType>String</dataType>
        <expression>CASE( {!Process_SKUs.SKU_Product__r.Product_Type__c},
&quot;Main&quot;, &quot;Users&quot;, 
&quot;Units&quot; )</expression>
    </formulas>
    <formulas>
