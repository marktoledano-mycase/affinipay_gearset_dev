    <description>Updates the Total Field when record is created or updated.</description>
    <environments>Default</environments>
    <formulas>
        <name>fxARRTotalChild</name>
        <dataType>Currency</dataType>
        <expression>({!Calculated_ARR_Total_Loop.Price_Base__c}  - 
(IF(ISBLANK({!Calculated_ARR_Total_Loop.Short_Term_Discount__c}),0,{!Calculated_ARR_Total_Loop.Short_Term_Discount__c} )* {!Calculated_ARR_Total_Loop.Price_Base__c}) -
(IF(ISBLANK({!Calculated_ARR_Total_Loop.Lifetime_Discount__c}),0,{!Calculated_ARR_Total_Loop.Lifetime_Discount__c}) * {!Calculated_ARR_Total_Loop.Price_Base__c}))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxCommissionedARRChild</name>
        <dataType>Currency</dataType>
        <expression>IF( ISPICKVAL({!Calculated_ARR_Total_Loop.SKU_Product__r.Pricing_Type__c},&quot;Monthly&quot;),12,1)
*
{!Calculated_ARR_Total_Loop.Price_Base__c} - 
(
IF( ISPICKVAL( {!Calculated_ARR_Total_Loop.Lifetime_Promotion__r.Short_Term_Discount_Type__c},&quot;Sales Approved&quot; ),{!Calculated_ARR_Total_Loop.Short_Term_Discount__c}, 
0 ) 
*
{!Calculated_ARR_Total_Loop.Price_Base__c}
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxExpectedARRTotalChild</name>
        <dataType>Currency</dataType>
        <expression>({!Calculated_ARR_Total_Loop.Price_Base__c}  - 
(IF(ISBLANK({!Calculated_ARR_Total_Loop.Short_Term_Discount__c}),0,{!Calculated_ARR_Total_Loop.Short_Term_Discount__c} )* {!Calculated_ARR_Total_Loop.Price_Base__c}) -
(IF(ISBLANK({!Calculated_ARR_Total_Loop.Lifetime_Discount__c}),0,{!Calculated_ARR_Total_Loop.Lifetime_Discount__c}) * {!Calculated_ARR_Total_Loop.Price_Base__c})) 
* {!$Record.Opportunity__r.Probability}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxExpectedCommissionedARRChild</name>
        <dataType>Currency</dataType>
        <expression>{!fxCommissionedARRChild} * {!$Record.Opportunity__r.Probability}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxMonthlyToAnnualARRTotalChild</name>
        <dataType>Currency</dataType>
        <expression>({!Calculated_ARR_Total_Loop.Price_Base__c}  - 
(IF(ISBLANK({!Calculated_ARR_Total_Loop.Short_Term_Discount__c}),0,{!Calculated_ARR_Total_Loop.Short_Term_Discount__c} )* {!Calculated_ARR_Total_Loop.Price_Base__c}) -
(IF(ISBLANK({!Calculated_ARR_Total_Loop.Lifetime_Discount__c}),0,{!Calculated_ARR_Total_Loop.Lifetime_Discount__c}) * {!Calculated_ARR_Total_Loop.Price_Base__c})) 
*12</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxMonthlytoAnnualExpectedARRTotalChild</name>
        <dataType>Currency</dataType>
        <expression>({!Calculated_ARR_Total_Loop.Price_Base__c}  - 
(IF(ISBLANK({!Calculated_ARR_Total_Loop.Short_Term_Discount__c}),0,{!Calculated_ARR_Total_Loop.Short_Term_Discount__c} )* {!Calculated_ARR_Total_Loop.Price_Base__c}) -
(IF(ISBLANK({!Calculated_ARR_Total_Loop.Lifetime_Discount__c}),0,{!Calculated_ARR_Total_Loop.Lifetime_Discount__c}) * {!Calculated_ARR_Total_Loop.Price_Base__c})) 
*12
* {!$Record.Opportunity__r.Probability}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxOpportunitySkuTotal</name>
        <dataType>Currency</dataType>
        <expression>If ({!$Record.Product_Sub_Family__c} = &#39;Soluno&#39; ,{!$Record.Total_All_Tiers__c} ,  {!$Record.Qty__c} * {!$Record.Price_Base__c} )</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fxParentOpportunityTotal</name>
        <dataType>Currency</dataType>
        <expression>{!Calculate_Parent_ARR_Loop.Qty__c} * {!Calculate_Parent_ARR_Loop.Price_Base__c}</expression>
        <scale>2</scale>
    </formulas>
